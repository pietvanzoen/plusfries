  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "newman": "^4.2.2",
    "nodemon": "^1.18.7",
    "prettier": "^1.15.3",
    "terser": "^3.11.0",
    "uglifycss": "0.0.29"
  },
