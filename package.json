{
  "name": "plusfries",
  "version": "0.3.1",
  "description": "+üçü as a service",
  "main": "app.js",
  "scripts": {
    "start:prod": "node app.js",
    "prestart": "npm run build",
    "start": "nodemon ./app.js",
    "start:test-server": "docker-compose -f ./tests/docker-compose.yml up -d --build --force-recreate test-server",
    "test:api": "CI=true newman run -n 3 -e tests/local.postman_environment.json tests/plusfries.postman_collection.json",
    "build": "./scripts/widget.sh",
    "lint": "eslint .",
    "fmt": "prettier --no-color --write '**/*.{md,js}'",
    "preinstall": "test -f .env || cp -n .env.example .env",
    "postinstall": "./scripts/git-hooks-install",
    "demo": "docker-compose -f ./demo/docker-compose.yml up -d && echo 'demo site running at http://localhost:4567'",
    "demo:rebuild": "docker-compose -f ./demo/docker-compose.yml build",
    "demo:stop": "docker-compose -f ./demo/docker-compose.yml down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pietvanzoen/plusfries.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pietvanzoen/plusfries/issues"
  },
  "homepage": "https://github.com/pietvanzoen/plusfries#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "cssnano": "^4.1.7",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "newman": "^4.2.2",
    "nodemon": "^1.18.7",
    "postcss-cli": "^6.0.1",
    "prettier": "^1.15.3",
    "terser": "^3.11.0"
  },
  "dependencies": {
    "colors": "^1.3.2",
    "dotenv-safe": "^6.1.0",
    "loglevel": "^1.6.1",
    "morgan": "^1.9.1",
    "restify": "^7.2.3",
    "restify-cors-middleware": "^1.1.1",
    "sequelize": "^4.41.2",
    "sqlite3": "^4.0.4"
  }
}
